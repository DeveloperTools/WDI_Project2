<link rel="stylesheet" href="/style/league.css">

<div id="page_header">
  <div id="container_logo">
    <img src="/img/logo_large.png" alt="" />
  </div>
</div>

<div class="container">
  <div class="row">
    <div class="six columns">
      <h3>Welcome back <%= session[:current_account] %>!</h3>
      <h4>Create a new league</h4>
      <h4>jump into a league draft you've been invited to</h4>
      <h4>or simulate a season for a league you've already joined!</h4>

      <h5><strong>You've been invited to draft in these leagues!</strong></h5>
      <div id="notDrafted">
      </div>

      <h5><strong>You're part of the following leagues</strong></h5>
      <div id="drafted">
      </div>

    </div>
  </div>
</div>
<script src="/script/league_ajax.js"></script>
<script src="/script/league.js"></script>
